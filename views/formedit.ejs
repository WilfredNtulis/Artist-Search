<!DOCTYPE html>
<html>
<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="public/assets/form.css">
</head>
<body>
<!--====form section start====-->


  <% if (message){ %>
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
      <%= message%>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
 <% } %>
  
  

<div class="artist-detail" id="top">
  <div class="jumbotron">
  <h1 class="display-4">Create Artist Data </h1>

  </div>

    <form action="/edit" method="POST">
      <div id="name" class="all">
          <label>Name :</label>
          <input type="text" placeholder="Enter Name" name="name" id="nameInPut" required>
      </div>
      <div id="surname" class="all">
        <label>Surname :</label>
        <input type="text" placeholder="Surname" name="surname" id="surnameInPut" required>
    </div>
      <div id="stagename" class="all">
          <label>Stagename :</label>
          <input type="text" placeholder="Stagename" name="stagename" id="stagenameInPut" required>
      </div>
      <div id="genre" class="all">
          <label>Genre :</label>
          <input type="text" placeholder="genre" name="genre" id="genreInPut" required>
      </div>
      <div id="province" class="all">
          <label>Province/City :</label>
          <input type="text" placeholder="Enter Province/City" name="province" id="provinceInPut" required>
      </div>
      <div id="country" class="all">
          <label>Email :</label>
          <input type="email" placeholder="email" name="email" id="emailInPut" required>
      </div>
      <div id="address" class="all">
        <label>Address :</label>
        <input type="text" placeholder="Address" name="address" id="addressInPut" required>
    </div>
    <div id="phone" class="all">
      <label>Phone number :</label>
      <input type="text" placeholder="Phone Number" name="phone" id="phoneInPut" required>
  </div>
      <div>
    <button type="submit" id="submitbtn"> Save</button>
      </div>
        </div>
</div>
</form>
<a href="/"> <button id="submitbtn"> Cancel</button></a>
<script>
 var person =  JSON.parse(window.localStorage.getItem('person'))
 window.onload = (event) => {
   console.log(person)
 }
</script>
</body>
</html>