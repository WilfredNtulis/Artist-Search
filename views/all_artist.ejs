<body>
<!--====form section start====-->
<nav><span class="whiteThing"><img src="https://urbanculxure.com/wp-content/uploads/2019/04/NACZ.jpg" id="image1"><span class="heading">Results</span></span></nav>
<div class="container">
<div id="result">Results(Nos of results)</div>
<% results.forEach(function(result){ %>
 <div class="dotted">
 	<div class="tops"><span id="full" class="labels"><%= result.name %></span><span id="stage" class="labels"><%= result.stagename %></span></div>
 	<div class="tops"><span id="genre" class="labels"> <%= result.genre%></span><span id="contact" class="labels"> <%= result.phonenumber%></span></div>
 	<div class="tops"><span id="province" class="labels"><%= result.province%></span><span id="email" class="labels">  <%= result.email%></span></div>
 </div>
 <%  }); %>
</div>
